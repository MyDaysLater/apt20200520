.apt-index .top-input {
  height: 100rpx;
  background-color: #f93553;
  display: flex;
  justify-content: center;
  text-align: center;
}
.apt-index .top-input navigator.input {
  border-radius: 25rpx;
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  color: #767676;
  background-color: white;
}
.apt-index .apt-nav {
  height: 100px;
  display: flex;
  align-items: center;
}
.apt-index .apt-nav image {
  flex: 1;
  height: 140rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.apt-index .nav .nav-title {
  color: pink;
  margin: 10rpx;
}
.apt-index .nav .nav-list {
  display: flex;
  flex-wrap: wrap;
}
.apt-index .nav .nav-list navigate {
  height: 188rpx;
  width: 30%;
  padding: 10rpx;
}
.apt-index .nav .nav-list navigate image {
  width: 100%;
  height: 100%;
}
